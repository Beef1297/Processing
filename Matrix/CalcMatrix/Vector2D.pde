
class Vector2D {
  
  float x;
  float y;
  
  public Vector2D() {}
  public Vector2D(float x_, float y_) {
    x = x_;
    y = y_;
  }
  
  public void printVector2D() {
    println("***************");
    println("x: " + x + " y: " + y);
    println("***************");
  }
  
}
